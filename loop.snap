; s += a * b

(def inner (fn (a s i m)
               (if (< i m) (recur a (add s (mul a i)) (add i 1) m) s)
               ))
; (print (inner 1 0 0 10000))
(def loop (fn (s i m)
              (if (< i m) (recur (inner i 0 s m) (add i 1) m) s)
     ))

(print (loop 0 0 1000))
